#!/bin/bash

# Enable verbose output for debugging
set -x

# Fetch Jenkins CLI with SSL debugging
curl -v -O https://jenkins-nts2.vpc.verizon.com/nts2/jnlpJars/jenkins-cli.jar

# Run Jenkins CLI with SSL handshake debugging
java -Djavax.net.debug=ssl:handshake -jar jenkins-cli.jar -s https://jenkins-nts2.vpc.verizon.com/nts2/ -auth SVC-GQUV-GIT-PROD:1145963a865c7667f58c4c39b0ad4c2c3b -http get-node NTS-GQUV-PORCH-INFRASTRUCTURE | java -jar jenkins-cli.jar -s https://jenkins-nts2.vpc.verizon.com/nts2 -auth SVC-GQUV-GIT-PROD:1145963a865c7667f58c4c39b0ad4c2c3b -http create-node $thishost

# Continue with the rest of the script...
